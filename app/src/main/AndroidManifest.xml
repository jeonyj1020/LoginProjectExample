<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginprojectexample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_knu"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_knu_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginProjectExample">
        <activity
            android:name=".SelectActivity"
            android:exported="true" />
        <activity android:name=".MyCurrentLaundryActivity" />
        <activity android:name=".WashingMachineActivity" />
        <activity android:name=".LaundryReservationActivity" />
        <activity android:name=".auth.PasswordResetActivity" />
        <activity android:name=".auth.LoginActivity" />
        <activity android:name=".auth.SignUpActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="google.com"
                    android:scheme="http" />
            </intent-filter>
        </activity>
    </application>

</manifest>